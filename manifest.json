{
  "manifest_version": 2,
  "name": "Auto Logout",
  "version": "0.1",

  "description": "This extension logs you out from site when you close tab",
  "icons": { "16": "icon16.png",
             "19": "icon19.png",
             "48": "icon48.png",
             "128": "icon128.png" },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
